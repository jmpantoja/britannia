{% macro pending(label = '') %}
    <span class="label label-warning label-round">{{ label }}</span>
{% endmacro %}

{% macro active(label = '') %}
    <span class="label label-success label-round">{{ label }}</span>

{% endmacro %}

{% macro finalized(label = '') %}
    <span class="label label-danger label-round">{{ label }}</span>
{% endmacro %}
