{% extends get_admin_template('base_list_field', admin.code) %}

{% block field %}
    <p>
        <strong><a href="{{ admin.generateObjectUrl('edit', object) }}">{{ object.fullName }}</a></strong>
    </p>
    <span style="font-style: italic; color: darkslategray">
        <i class="fa fa-user"></i>
        {{ object.userName }}
    </span>
{% endblock %}

