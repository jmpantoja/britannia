{% extends '@SonataAdmin/CRUD/base_list_field.html.twig' %}

{% use 'admin/core/status_column.html.twig' %}

{% block field %}
    {% if object.status.pending %}
        {{ block('pending') }}
    {% elseif object.status.active %}
        {{ block('active') }}
    {% else %}
        {{ block('finalized') }}
    {% endif %}
{% endblock %}



