<?php

namespace Britannia\Domain\Entity\Student;

use PlanB\DDD\Domain\VO\DNI;

class Adult extends Student
{

    /**
     * @var DNI
     */
    private $dni;


    /**
     * @return DNI
     */
    public function getDni(): ?DNI
    {
        return $this->dni;
    }

    /**
     * @param DNI $dni
     * @return Student
     */
    public function setDni(?DNI $dni): self
    {
        $this->dni = $dni;
        return $this;
    }

}
