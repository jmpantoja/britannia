{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "api-platform/api-pack": "^1.1",
        "beberlei/doctrineextensions": "^1.2",
        "cocur/slugify": "^3.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "knplabs/knp-snappy-bundle": "^1.6",
        "league/tactician": "^1.0",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "marc-mabe/php-enum": "^4.1",
        "mikehaertl/php-pdftk": "^0.8.0",
        "planb/ddd": "*@dev",
        "planb/ddd-bundle": "*@dev",
        "ramsey/uuid-doctrine": "^1.5",
        "sonata-project/admin-bundle": "^3.53",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/formatter-bundle": "^4.1",
        "sonata-project/intl-bundle": "^2.7",
        "sonata-project/translation-bundle": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/maker-bundle": "^1.13",
        "symfony/monolog-bundle": "^3.4",
        "symfony/yaml": "4.3.*",
        "tightenco/collect": "^6.5",
        "zendframework/zend-filter": "^2.9"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "fzaninotto/faker": "^1.8",
        "symfony/var-dumper": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Britannia\\": "src/Britannia"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Britannia\\Tests\\": "tests/Britannia"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "egeloen/ckeditor-bundle": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./libs/planb/ddd-bundle",
            "symlink": true
        },
        {
            "type": "path",
            "url": "./libs/planb/ddd",
            "symlink": true
        }
    ]
}
